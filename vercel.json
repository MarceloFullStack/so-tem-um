{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/upload",
      "methods": ["POST"],
      "dest": "api/upload.js"
    }
  ],
  "files": {
    "public/images/*": {
      "write": true
    },
    "data.json": {
      "write": true
    }
  }
}